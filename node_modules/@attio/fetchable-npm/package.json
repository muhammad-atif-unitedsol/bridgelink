{
  "name": "@attio/fetchable",
  "sideEffects": false,
  "version": "0.0.1-experimental.8",
  "main": "./lib/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./lib/index.js",
      "default": "./lib/index.cjs"
    }
  },
  "files": [
    "lib"
  ],
  "type": "module",
  "scripts": {
    "build": "tsup",
    "build-tsc": "tsc --build",
    "build-tsc-watch": "tsc --build --watch",
    "test": "yarn run test-typescript && yarn run test-vitest",
    "test-vitest": "vitest run",
    "test-typescript": "tsc --build",
    "lint": "yarn run -T eslint --quiet '{src,tests}/**/*.{js,ts,tsx}'",
    "lint-fix": "yarn run lint --fix",
    "format": "yarn run -T prettier --ignore-path .prettierignore --check ./",
    "format-fix": "yarn run format --write ./",
    "clean": "rm -rf lib && rm -rf tsconfig.tsbuildinfo",
    "check-exports": "attw --pack ."
  },
  "devDependencies": {
    "typescript": "5.6.3",
    "expect-type": "^1.2.0",
    "@arethetypeswrong/cli": "^0.17.4",
    "tsup": "^8.4.0",
    "prettier": "^3.5.3",
    "vitest": "^3.0.8",
    "eslint": "^9.22.0",
    "@eslint/js": "^9.22.0",
    "typescript-eslint": "^8.26.1"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
